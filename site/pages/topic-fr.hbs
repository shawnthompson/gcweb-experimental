---
{
	"title": "[Nom du sujet]",
	"language": "fr",
	"altLangPrefix": "topic",
	"pageclass": "secondary",
	"breadcrumb": [
		{ "title": "Accueil", "link": "http://www.canada.ca/fr/index.html" },
		{ "title": "[Thème]", "link": "theme-fr.html" }
	],
	"dateModified": "2016-01-18",
	"share": "true",
	"deptfeature": true,
	"pagetag": "1 ou 2 phrases d’introduction qui définissent les sous-sujets et les tâches principales qui peuvent être consultés sur cette page."
}
---
<div class="row profile">
	<div class="col-md-6">
		<h1 property="name" id="wb-cont">{{{title}}}</h1>
		<p class="pagetag">{{pagetag}}</p>
		{{>follow}}
	</div>
	<div class="col-md-6 margin-top-small hidden-sm hidden-xs">
		<img src="{{assets}}/img/520x200.png" alt="" class="pull-right img-responsive thumbnail"/>
	</div>
</div>
<div class="row">
	<div class="col-md-4 mrgn-tp-lg pull-right">
		{{>most-requested}}
	</div>
	<section class="col-md-8 pull-left mrgn-bttm-lg">
		<h2>Services et renseignements</h2>
		<div class="row">
			{{>topic-subtopic}}
			{{>topic-subtopic}}
			<div class="clearfix"></div>
			{{>topic-subtopic}}
			{{>topic-subtopic}}
			<div class="clearfix"></div>
			{{>topic-subtopic}}
			{{>topic-subtopic}}
			<div class="clearfix"></div>
			{{>topic-subtopic}}
			{{>topic-subtopic}}
			<div class="clearfix"></div>
			{{>topic-subtopic}}
			{{>topic-subtopic}}
			<div class="clearfix"></div>
			<details class="mrgn-tp-md brdr-0">
				<summary class="btn btn-default text-center">Afficher plus de services et de renseignements</summary>
				<div class="row">
					{{>topic-subtopic}}
					{{>topic-subtopic}}
					<div class="clearfix"></div>
					{{>topic-subtopic}}
					{{>topic-subtopic}}
					<div class="clearfix"></div>
					{{>topic-subtopic}}
					{{>topic-subtopic}}
					<div class="clearfix"></div>
				</div>
			</details>
		</div>
	</section>
	<div class="clr-rght-md clr-rght-lg"></div>
	<div class="pull-right col-md-4">
		{{>supported-by}}
		{{>more-info}}
	</div>
</div>
<section class="whtwedo">
	<h2>Ce que nous faisons</h2>
	<div class="row wb-eqht">
		<section class="col-lg-4 col-md-6">
			{{>law-regulations}}
		</section>
		<section class="col-lg-4 col-md-6">
			{{>publications}}
		</section>
		<section class="col-lg-4 col-md-6">
			{{>consultations}}
		</section>
		<section class="col-lg-4 col-md-6">
			{{>open-data}}
		</section>
		<section class="col-lg-4 col-md-6">
			{{>statistics}}
		</section>
	</div>
</section>
